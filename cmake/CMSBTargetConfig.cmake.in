set(${__CMSB_LIBNAME}_COMPILE_DEFINITIONS ${${__CMSB_LIBNAME}_COMPILE_DEFINITIONS})

set(CMAKE_CXX_STANDARD @CMAKE_CXX_STANDARD@)
